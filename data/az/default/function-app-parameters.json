{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "$(location)"
        },
        "storageAccountName": {
            "value": "clientxstorageaccount"
        },
        "accountType": {
            "value": "Standard_LRS"
        },
        "kind": {
            "value": "StorageV2"
        },
        "accessTier": {
            "value": "Hot"
        },
        "minimumTlsVersion": {
            "value": "TLS1_2"
        },
        "supportsHttpsTrafficOnly": {
            "value": true
        },
        "allowBlobPublicAccess": {
            "value": false
        },
        "allowSharedKeyAccess": {
            "value": true
        },
        "defaultOAuth": {
            "value": false
        },
        "networkAclsBypass": {
            "value": "AzureServices"
        },
        "networkAclsDefaultAction": {
            "value": "Deny"
        },
        "networkAclsVirtualNetworkRules": {
            "value": [
                {
                    "id": "/subscriptions/$(subscription)/resourceGroups/Peregrine-QA/providers/Microsoft.Network/virtualNetworks/qa-peregrine-vnet/subnets/default"
                }
            ]
        },
        "keySource": {
            "value": "Microsoft.Storage"
        },
        "encryptionEnabled": {
            "value": true
        },
        "infrastructureEncryptionEnabled": {
            "value": true
        },
        "isContainerRestoreEnabled": {
            "value": false
        },
        "isBlobSoftDeleteEnabled": {
            "value": true
        },
        "blobSoftDeleteRetentionDays": {
            "value": 7
        },
        "isContainerSoftDeleteEnabled": {
            "value": true
        },
        "containerSoftDeleteRetentionDays": {
            "value": 7
        },
        "changeFeed": {
            "value": false
        },
        "isVersioningEnabled": {
            "value": false
        },
        "isShareSoftDeleteEnabled": {
            "value": true
        },
        "shareSoftDeleteRetentionDays": {
            "value": 7
        }
    }
}