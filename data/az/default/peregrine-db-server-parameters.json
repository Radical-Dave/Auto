{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "allowAzureIps": {
            "value": true
        },
        "administratorLogin": {
            "value": "$(peregrine-db-server-login)"
        },
        "administratorLoginPassword": {
            "value": "$(peregrine-db-server-password)"
        },
        "enableADS": {
            "value": false
        },
        "serverName": {
            "value": "$(prefix)-$(envName)-peregrine-db"
        },
        "enableVA": {
            "value": false
        },
        "useVAManagedIdentity": {
            "value": false
        },
        "serverTags": {
            "value": {}
        }
    }
}